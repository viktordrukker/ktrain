{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2021",
    "checkJs": true,
    "allowJs": true,
    "strict": false,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", "data", "backups"]
}
